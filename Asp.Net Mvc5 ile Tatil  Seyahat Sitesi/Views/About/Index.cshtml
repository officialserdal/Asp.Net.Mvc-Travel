@using Asp.Net_Mvc5_ile_Tatil__Seyahat_Sitesi.Models.Sınıflar
@model List<Hakkimizda>
    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_TestLayout.cshtml";
    }

    <div class="content">
        <div class="wrap"> 
            <div class="about">
                <h3>Hakkımızda</h3> 
                @foreach (var x in Model)
                {
                    <img src="@x.Fotourl" alt="" />

                    <p>@x.Açıklama</p>
                }

            </div>
        </div>
    </div>

