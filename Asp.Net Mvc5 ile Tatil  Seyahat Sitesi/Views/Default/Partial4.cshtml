@using Asp.Net_Mvc5_ile_Tatil__Seyahat_Sitesi.Models.Sınıflar
@model  List<Blog>


<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {

            <div class="bot-img">


                <img src="@x.BlogImage" alt="" />
            </div>
            <div class="bot-desc">
                <p>@x.Acıklama.Substring(0,70)...</p>
            </div>
            <div class="clear"></div>


        }
    </div>
    @Html.Action("Partial5", "Default")
</div>
